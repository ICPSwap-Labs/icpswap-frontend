{
  "name": "@icpswap/frontend",
  "version": "4.3.1",
  "description": "Frontend of ICPSwap",
  "scripts": {
    "dev": "node ./scripts/prepare.js && turbo run dev --concurrency 100",
    "start:local": "turbo run start:local",
    "build": "node ./scripts/prepare.js && turbo run build",
    "build:swap": "node ./scripts/prepare.js && turbo run build --filter=swap --no-cache --force",
    "build:info": "node ./scripts/prepare.js && turbo run build --filter=info --no-cache --force",
    "deploy:info:ic": "node ./scripts/prepare.js && turbo run deploy:ic --filter=info",
    "deploy:swap:ic": "node ./scripts/prepare.js && turbo run deploy:ic --filter=swap",
    "deploy:release": "node ./scripts/prepare.js && turbo run deploy:release",
    "deploy:swap:release": "node ./scripts/prepare.js && turbo run deploy:release --filter=swap",
    "deploy:info:release": "node ./scripts/prepare.js && turbo run deploy:release --filter=info",
    "deploy:swap:test": "node ./scripts/prepare.js && turbo run deploy:test --filter=swap",
    "deploy:info:test": "node ./scripts/prepare.js && turbo run deploy:test --filter=info",
    "clean": "turbo run clean && rm -rf node_modules",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "node ./scripts/prepare.js",
    "fetch-tokens": "ts-node-esm ./scripts/pre-fetch-tokens.ts"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^13.13.5",
    "jest": "26.6.0",
    "turbo": "1.10.15"
  },
  "dependencies": {
    "@dfinity/agent": "1.0.1",
    "@dfinity/auth-client": "1.0.1",
    "@dfinity/candid": "1.0.1",
    "@dfinity/identity": "1.0.1",
    "@dfinity/ledger-icp": "2.2.0",
    "@dfinity/principal": "1.0.1",
    "@dfinity/utils": "2.1.1",
    "@mui/base": "5.0.0-beta.27",
    "@mui/icons-material": "5.5.0",
    "@mui/lab": "5.0.0-alpha.72",
    "@mui/material": "5.5.0",
    "@mui/styles": "5.5.0",
    "bignumber.js": "^9.0.1",
    "copy-to-clipboard": "^3.2.0",
    "jsbi": "^4.3.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "tiny-invariant": "^1.3.1"
  },
  "author": "",
  "license": "MIT License",
  "engines": {
    "node": ">=18.x",
    "pnpm": ">=8.x"
  }
}
